<style>


    video {
        // width: 100%;
        margin-bottom: 10px;
    }


    h1 {
        font-family: 'Komika Hand', sans-serif;
    }

    h2 {
        padding: 0px 100px;
    }

    .row {
        width: 100%;
        margin: 0px;
    }

    #video-row {
        padding: 50px;
    }

    .col-lg-6 {
        padding: 30px;
    }

    body {
        margin: 0;
        background: #000 !important;
    }
</style>
<link href="~/Content/Style.css" rel="stylesheet" />
<!-- Video header -->
<section>

    <video id="video-box" autoplay muted loop>
        <source src="https://drive.google.com/uc?export=download&id=1EUDR3uwTigEz9kmvpZRQ6kU1JcXS5ljh" type="video/mp4">
        Your browser does not support HTML5 video.
    </video>


    <div class="content">
        <h1 id="video-tilte" style="font-family: 'Komika Hand', sans-serif;">Videos</h1>
        <div id="text-header" style="font-family: 'Raleway', sans-serif;">
            Want to know more about our impact on water catchment and how we can help saving our
            water?
        </div>
        <div id="text-header1">Scroll down to see more videos</div>

    </div>

    <!-- Video list may be removed later -->
    <div class="row">
        <div class="col-4">
            <ul class="video-list">
            </ul>
        </div>
    </div>

</section>
<!-- End Video header -->

<body>

    <div class="row " id="video-row" style=" background-color:rgb(229,229,229)">
        <div class="col-12 ">

            <h2 class="col-12" style="font-size: xx-large;font-weight: bold;color:black; text-align:center; font-family:'Komika Hand', sans-serif; color:#348172; margin:40px 0px;  ">Get to know more about our impact on water catchment through videos!</h2>

            <!-- Water Consumption -->
            <p style="font-family: 'Raleway', sans-serif; position: relative; color: burlywood; font-size: 2em; text-align: center; margin-top: 2em !important; margin-bottom: 2em !important;">Problems and Solutions of Water Consumption</p>

            <!-- Carousel section 1 -->
            <div class="row">

                <div class="col-12">

                    <div class="col-lg-6 col-md-12" style="margin-bottom:50px;">
                        <h3 style="text-align:center; font-weight:bold;">Fresh water scarcity</h3>
                        <div class="embed-responsive embed-responsive-16by9">

                            <iframe width="560" height="315" src="https://www.youtube.com/embed/otrpxtAmDAk"
                                    frameborder="0"
                                    allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture"
                                    allowfullscreen></iframe>
                        </div>
                    </div>

                    <div class="col-lg-6 col-md-12" style="margin-bottom:50px;">
                        <h3 style="text-align:center; font-weight:bold;">Are we running out of clean water?</h3>
                        <div class="embed-responsive embed-responsive-16by9">

                            <iframe width="560" height="315" src="https://www.youtube.com/embed/OCzYdNSJF-k"
                                    frameborder="0"
                                    allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture"
                                    allowfullscreen></iframe>
                        </div>
                    </div>





                </div>

            </div>

            <div class="row">

                <div class="col-12">

                    <div class="col-lg-6 col-md-12" style="margin-bottom:50px;">
                        <h3 style="text-align:center; font-weight:bold;">Melbourne's water story</h3>
                        <div class="embed-responsive embed-responsive-16by9">
                            <iframe width="560" height="315" src="https://www.youtube.com/embed/wjF8tux0Azg"
                                    frameborder="0"
                                    allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture"
                                    allowfullscreen>
                            </iframe>
                        </div>
                    </div>

                    <div class="col-lg-6 col-md-12" style="margin-bottom:50px;">
                        <h3 style="text-align:center; font-weight:bold;">The Global Water Crisis, Explained</h3>
                        <div class="embed-responsive embed-responsive-16by9">
                            <iframe width="560" height="315" src="https://www.youtube.com/embed/vB68xvRb2T4"
                                    frameborder="0"
                                    allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture"
                                    allowfullscreen></iframe>
                        </div>
                    </div>
                </div>

            </div>

            <div class="row">

                <div class="col-12">

                    <div class="col-lg-6 col-md-12" style="margin-bottom:50px;">
                        <h3 style="text-align:center; font-weight:bold;">
                            Water saving tips from Andrew Hansen
                        </h3>
                        <div class="embed-responsive embed-responsive-16by9">
                            <iframe width="560" height="315" src="https://www.youtube.com/embed/-UyjJztcazU"
                                    frameborder="0"
                                    allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture"
                                    allowfullscreen>
                            </iframe>
                        </div>
                    </div>

                    <div class="col-lg-6 col-md-12" style="margin-bottom:50px;">
                        <h3 style="text-align:center; font-weight:bold;">Top 10 water saving tips</h3>
                        <div class="embed-responsive embed-responsive-16by9">
                            <iframe width="560" height="315" src="https://www.youtube.com/embed/5J3cw4biWWo"
                                    frameborder="0"
                                    allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture"
                                    allowfullscreen></iframe>
                        </div>
                    </div>
                </div>

            </div>




            <div style="text-align: right; font-size: 12px; color: black; opacity: 0.7">
                Sources: Our Changing Climate and
                National Center for Science Education, TED-Ed, Melbourne Water
            </div>
            <!-- End Carousel section 1 -->
        </div>
    </div>

    <div class="row" style="background-color:white">
        <div class="col col-md-10 offset-md-1 col-lg-8 offset-lg-2">
            <!-- Water Pollution -->
            <p style="font-family: 'Raleway', sans-serif; position: relative; color: burlywood; font-size: 2em; text-align: center; margin-top: 2em !important; margin-bottom: 2em !important; ">Problems and Solutions of Water Pollution</p>

            <!-- Carousel section 2 -->
            <div id="carousel-section-2" class="carousel slide" data-ride="false">
                <ol class="carousel-indicators">
                    <li data-target="#carousel-section-2" data-slide-to="0" class="active"></li>
                    <li data-target="#carousel-section-2" data-slide-to="1"></li>
                    <li data-target="#carousel-section-2" data-slide-to="2"></li>
                    <li data-target="#carousel-section-2" data-slide-to="3"></li>
                    <li data-target="#carousel-section-2" data-slide-to="4"></li>
                    <li data-target="#carousel-section-2" data-slide-to="5"></li>
                </ol>
                <div class="carousel-inner">
                    <div class="carousel-item active">
                        <div class="embed-responsive embed-responsive-16by9">
                            <iframe width="560" height="315" src="https://www.youtube.com/embed/qM8uvAZ2wU0"
                                    frameborder="0"
                                    allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture"
                                    allowfullscreen></iframe>
                        </div>
                    </div>
                    <div class="carousel-item">
                        <div class="embed-responsive embed-responsive-16by9">
                            <iframe width="560" height="315" src="https://www.youtube.com/embed/HQTUWK7CM-Y"
                                    frameborder="0"
                                    allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture"
                                    allowfullscreen></iframe>
                        </div>
                    </div>
                    <div class="carousel-item">
                        <div class="embed-responsive embed-responsive-16by9">
                            <iframe width="560" height="315" src="https://www.youtube.com/embed/Zk1J2EW-nmQ"
                                    frameborder="0"
                                    allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture"
                                    allowfullscreen></iframe>
                        </div>
                    </div>
                    <div class="carousel-item">
                        <div class="embed-responsive embed-responsive-16by9">
                            <iframe width="560" height="315" src="https://www.youtube.com/embed/dmDsIvCI10Q"
                                    frameborder="0"
                                    allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture"
                                    allowfullscreen>
                            </iframe>
                        </div>
                    </div>
                    <div class="carousel-item">
                        <div class="embed-responsive embed-responsive-16by9">
                            <iframe width="560" height="315" src="https://www.youtube.com/embed/U8y-s3IYvaY"
                                    frameborder="0"
                                    allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture"
                                    allowfullscreen>
                            </iframe>
                        </div>
                    </div>
                    <div class="carousel-item">
                        <div class="embed-responsive embed-responsive-16by9">
                            <iframe width="560" height="315" src="https://www.youtube.com/embed/TZ_AQxALm0I" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
                        </div>
                    </div>
                </div>
                <a class="carousel-control-prev" href="#carousel-section-2" role="button" data-slide="prev"
                   style=" height: 26%; top: 40%; border-radius: 100px;">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                </a>
                <a class="carousel-control-next" href="#carousel-section-2" role="button" data-slide="next"
                   style=" height: 26%; top: 40%; border-radius: 100px;">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                </a>
            </div>
            <div style="text-align: center; font-size: 12px; color: white; opacity: 0.7; margin:50px 0px;">
                Sources: Melbourne Water, Queensland Government,
                Guardian Australia, National Geographic, FuseSchool - Global Education
            </div>
            <!-- End Carousel section 2 -->

        </div>
    </div>
</body>

<!-- Javascript -->



<script src="~/Scripts/video-app.js"></script>


